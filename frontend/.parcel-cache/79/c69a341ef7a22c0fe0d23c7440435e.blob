  <br><br>
    <div id="form" class="relative">
        
        <h1>Add your Resume Details</h1>
        <div class="form">
            <form>
              <label style="9b8fe8f1a7f0079c97172a0c8c16f7f2">Name</label>
              <br>
              <input type="text" id="name" placeholder="Candidate Name.." required="" title="*Must required">
              <br>
              <label style="1931ddda3279e7825fb48be9de0f7a27">College Name</label>
              <br>
              <input type="text" id="cname" class="cname" placeholder="College Name.." title="Please include correct Details" required="">
              <br>
              <label style="2870be549b006b0b9462a70c3a5a0271" for="degree">Degree</label><br>
                <select id="degree">
                <option value="B.Tech">B.Tech</option>
                <option value="B.com">B.com</option>
                <option value="BE">BE</option>
                <option value="BCA">BCA</option>
                <option value="BBA">BBA</option>
                <option value="BA">BA</option>
                <option value="BSc">BSc</option>
                <option value="BEd">BEd</option>
                <option value="IMSc">IMSc</option>
                <option value="MCA">MCA</option>
               </select>
              <br>
              <label style="dd353c3f15489cc01f98b457b44acb51" for="marks">Marks in 12th</label><br>
              <input type="text" id="marks" class="marks" placeholder="Marks in 12th">
              <br>
              <label style="e023f4a0165ee7cde2a1809185271c92" for="cgpa">CGPA / Percentage in Graduation</label><br>
              <input type="text" id="gpa" placeholder="CGPA/Percentage ..">
              <br>
              <label style="4d4948c2f981c360da0ffde7ecd6a88a" for="skill">Technical Skills</label><br>
              <select id="skill">
               
                <option value="Anthropology">Anthropology</option>
                <option value="Agriculture">Agriculture</option>
                <option value="Biotechnology">Biotechnology</option>
                <option value="Computer Science">Computer Science</option>
                <option value="Economics">Economics</option>
                <option value="Environmental Management">Environmental Management</option>
                <option value="Human Geography">Human Geography</option>
                <option value="Journalism">Journalism</option>
                <option value="Mechanical Engineer">Mechanical Engineer</option>
                <option value="Electrical and Eelctronics">Electrical and Eelctronics</option>
                <option value="Food Technology">Food Technology</option>
                <option value="Marine Engineering">Marine Engineering</option>
               </select>
              <br>
              <button type="submit" id="hellobutton">Submit</button>
            </form>
            <p id="demo" class="para">
             <button type="submit"><a href="979e73594347ab2648f084394fccab4f">Find Jobs</a></button>
             
            </p>
          </div>
    </div>
