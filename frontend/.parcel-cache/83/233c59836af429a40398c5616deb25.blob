  </ul>
  <br><br>
    <div class="relative">
        
        <h1>Add your Resume Details</h1>
        <div>
            <form>
              <label>Candidate Name</label>
              <br>
              <input type="text" id="name" placeholder="Candidate Name..">
              <br>
              <label>College Name</label>
              <br>
              <input type="text" id="cname" class="cname" placeholder="College Name..">
              <br>
              <label for="country">Degree</label><br>
              <input type="text" id="degree" class="degree" placeholder="Degree..">
              <br>
              <label for="country">Marks in 12th</label><br>
              <input type="text" id="marks" class="marks" placeholder="Marks in 12th">
              <br>
              <label for="country">CGPA / Percentage in Graduation</label><br>
              <input type="text" id="gpa" placeholder="CGPA/Percentage ..">
              <br>
              <label for="country">Technical Skills</label><br>
             
			  <select>
  <option value="Tony">Iron Man</option>
  <option value="Steve">Captain America</option>
</select>
              <button type="submit" id="hellobutton">Submit</button>
            </form>
            <p id="demo" class="para">
             <button type="submit"><a href="979e73594347ab2648f084394fccab4f">Find Jobs</a></button>
             
            </p>
          </div>
    </div>
    <script data-parcel-key="89296faccd7beb60d4a39fa06994727c">
      function display() {
        event.preventDefault();
        document.getElementById("demo").style.visibility = "visible";  
        var name = document.getElementById("name").value;
        var cname = document.getElementById("cname").value;
        var deg = document.getElementById("degree").value;
        var marks = document.getElementById("marks").value;
        var gpa = document.getElementById("gpa").value;
        var skill = document.getElementById("skill").value;
       
     document.getElementById("demo").innerHTML = name+"<br>\ "+cname+
     "<br>\ "+deg+"<br>\ Marks in 12th "+marks+"<br>\ "+ gpa+"CGPA <br>\ "+skill+" <br>\ "+ document.getElementById("demo").innerHTML;
     document.getElementById("demo").style.color = "blue";  

    }
      document.getElementById("hellobutton").addEventListener("click", display);
      </script>
    
</body>

</html>