body {margin:0;
  }
  h1{
    size: 200 px;
  }
  ul {
    list-style-type: none;
    margin: 0 0 3px 0;
    padding: 0;
    overflow: hidden;
    background-color: #333;
    position: fixed;
    top: 0;
    width: 100%;
  }
  li {
    float: left;
    margin: 0 0 3px 0;
    padding: 5px ;
  }
  li a {
    display: block;
    color: white;
    text-align: center;
    padding: 14px 16px;
    text-decoration: none;
  }
  li a:hover:not(.active) {
    background-color: #111;
  }
  .active {
    background-color:  rgb(43, 36, 34);
  }
  footer {
  text-align: center;
  padding: 3px;
  background-color:#333;
  color: white;
  
}
  @import url('http://fonts.cdnfonts.com/css/east-octopus---personal-use');